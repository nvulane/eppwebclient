      <%= form_for(:requested_domain, :url => {:action => 'requesting_transfer'}, :html => { :class => "form-horizontal form-condensed"}) do |f|%>
          <div class="control-group">
            <label class="control-label" for="first_name">Domain Name</label>
            <div class="controls">
              <%= f.text_field :reqdomain, :class => "large span9 required", :placeholder => "e.g example.co.za" %>
            </div>
          </div>
           <div class="control-group">
            <label class="control-label" for="debtor_code">Customer</label>
            <div class="controls">
              <%= f.text_field :debtor_code, :class => "span9 required", :id => "cus", :placeholder => "e.g something", 'AUTOCOMPLETE' => "off", 'data-provide'=>"typeahead", 'data-items'=>"4", 'data-source'=> @autocomplete_customer.to_json %>
            </div>
          </div>
          <div class="control-group">
            <label class="control-label" for="first_name"></label>
            <div class="controls">
              <button class="btn-info btn btn-xlarge" type="submit">Request</button>
            </div>
          </div>
        <%end%>
