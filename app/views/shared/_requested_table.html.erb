<%= render 'shared/search_form', :locals => { :action_name => "domains" }%>
<table id="domtable" class="table table-hover">
	<thead>
		<tr>
			    <th>Domain Name</th>
          <th>Customer</th>
          <th>Status</th>
          <th>Issued</th>
          <th>Vote</th>
		</tr>
	</thead>
  	<tbody>
  		<% @reqdomains.each do |d|%>
  		<tr>
  			<td><%= d.reqdomain %></td>
        <td><%= d.debtor_code %></td>
        <td><%= d.status %></td>
        <td><%= d.issue %></td>
        <td><%= d.vote %></td>
  		</tr>
  		<% end -%>
  	</tbody>
</table>
<%= will_paginate @reqdomains, :class => 'dmns'%>
